
<%- include('nav.ejs') %>
<div class="container-fluid bg-white p-3">
    <div class="row">
        <div class="col-12">
            <h1 class="text-center bg-warning text-dark font-weight-bold shadow"> Contact us Form </h1>
        </div>
    </div>
</div>  

<div class="container">
    <div class="row">
    <form action="/admin/savecontact_us" method="post" enctype="multipart/form-data">
           
               <div class="col-md-6 mt-3 float-left">
                <label>Enter Title 1</label>
                <input type="text" value="<%= contact.haed1 %>" name="haed1" class="form-control" >
               </div>
               <div class="col-md-6 mt-3 float-left">
                <label>Enter Title 2</label>
                <input type="text"  value="<%= contact.head2 %>"   name="head2" class="form-control" >
               </div>
               <div class="col-md-12 mt-3 float-left">
                <label>Office Address</label>
                <input type="text" value="<%= contact.office_address %>"  name="office_address" class="form-control" >
               </div>
               <div class="col-md-12 mt-3 float-left">
               <label>Phone Number</label>
               <input type="text"  value="<%= contact.pnumber %>"   name="pnumber" class="form-control" >
              </div>
              <div class="col-md-12 mt-3 float-left">
              <label>Our Email</label>
              <input type="text"  value="<%= contact.email %>"    name="email" class="form-control" >
              <input type="hidden"  value="<%= contact.contact_us_id %>"    name="contact_us_id" class="form-control" >
            </div>
       
           
               
               <div class="col-md-12 text-right float-left mt-4 mb-4">
                <button class="btn btn-primary">Update</button>
               </div>
            </form>      

    </div>
</div>

<%- include('footer.ejs') %>     

