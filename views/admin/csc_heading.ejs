<%- include('nav.ejs') %>
<div class="container-fluid bg-white p-3">
    <div class="row">
        <div class="col-12">
            <h1 class="text-center bg-warning text-dark font-weight-bold shadow">Csc Heading</h1>
        </div>
    </div>
</div>  
<div class="container">
  
    <form action="/admin/save_cschead" method="post" enctype="multipart/form-data">
     <div class="row">
        <div class="col-md- mt-3 float-left">   
            <input type="hidden" value="<%= head.csc_head_id %>"  name="csc_head_id">
            </div>
            <div class="col-md-4 mt-3 float-left">   
            <label>Head First</label>
            <input type="text" value="<%= head.first_head %>"  name="first_head" class="form-control">
            </div>
            <div class="col-md-4 mt-3 float-left">
            <label>Second Home</label>
             <input type="text" value="<%= head.shead %>" name="shead" class="form-control">
            </div>
            <div class="col-md-4 mt-3 float-left">
            <label>Third Head</label>
            <input type="text"  value="<%= head.tirdhead %>"   name="tirdhead" class="form-control">
           </div>
        
           <div class="col-md-12 text-right mt-4 mb-4">
            <button class="btn btn-primary ">Save</button>
           </div>
     </div>
    </form>      
</div>  
<%- include('footer.ejs') %>     

