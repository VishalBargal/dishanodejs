
<%- include('nav.ejs') %>
<div class="container-fluid bg-white p-3">
    <div class="row">
        <div class="col-12">
            <h1 class="text-center bg-warning text-dark font-weight-bold shadow"> Admissions </h1>
        </div>
    </div>
</div>  



<div class="container">
    <div class="row">
        <div class="col-md-12">
                <table class="table table-bordered table-hover table-striped text-dark">
                 <tr class="bg-success">
                    <th>Action</th>
                    <th>Sr no.</th>
                    <th>Name</th>
                    <th>date</th>
                 </tr>
                 <% for(i=0;i<admission.length;i++) { %> 
                    <tr>
                         <td>
                            <a href="/admin/delete_admissionform?admissionform_id=<%= admission[i].admissionform_id %>">
                                <button class="btn btn-danger btn-sm"><i class="fa fa-trash"></i></button>
                            </a>
                            <a href="/admin/details_admissionform?admissionform_id=<%= admission[i].admissionform_id %>">
                                <button class="btn btn-primary btn-sm">View</button>
                            </a>
                        </td>
                        <td><%= i+1 %></td>
                       <td>
                         <%= admission[i].fname %>
                        <%= admission[i].mname %>
                        <%= admission[i].lname %>
                        </td>
                        <td><%= admission[i].date %></td>
                     </tr>
                 <% } %>
                </table>
        </div>
       
    </div>
</div>




<%- include('footer.ejs') %>     

